@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    ViewData["Title"] = "แจ้งงานรถยนต์ (ปีแรก)";
    var fileExtensions = ".jpg,.jpeg,.pdf,.png";
}

@section Styles {
    <style>
        tr.dtrg-start {
            cursor: pointer;
            background-color: #f0f0f0;
            font-weight: bold;
        }
    </style>

    <!-- DataTables + Bootstrap 5 -->
    <link href="https://cdn.datatables.net/2.3.2/css/dataTables.bootstrap5.min.css" rel="stylesheet" />
    <!-- RowReorder -->
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/rowreorder/1.5.0/css/rowReorder.bootstrap5.min.css" />
    <!-- Responsive -->
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/responsive/3.0.4/css/responsive.bootstrap5.min.css" />
    <!-- RowGroup -->
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/rowgroup/1.5.1/css/rowGroup.bootstrap5.min.css">
    <!-- FixedHeader -->
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/fixedheader/4.0.2/css/fixedHeader.bootstrap5.min.css">

}

<header>
    <div class="row row-cols-md-1 my-3 justify-content-center align-items-center">
        <div class="offset-xxl-1 col-xxl-2 col-lg-3 col-md-4 col-5 mb-lg-0 mb-3">
            <div class="justify-content-center align-items-center">
                <img src="~/image/logo-company/tbroker_header.png" class="rounded img-fluid" alt="T Broker Header" asp-append-version="true" />
            </div>
        </div>
        <div class="col-xxl-6 col-lg-6 mb-lg-0 mb-3">
            <div class="text-center">
                <h1><span class="badge text-bg-secondary">@ViewData["Title"]</span></h1>
            </div>
        </div>
        <div class="col-xxl-3 col-lg-3">
            <div class="row justify-content-center align-items-center">
                <label for="agentCodeInput" class="col-lg-3 col col-3 col-form-label col-form-label-sm fw-bold">รหัสผู้ขาย</label>
                <div class="col-lg-9 col-9">
                    <input type="text" readonly class="form-control-plaintext" id="agentCodeInput" name="agentCodeInput" placeholder="รหัสผู้ขาย" value="@ViewBag.AgentCode">
                </div>
            </div>
            <div class="row justify-content-center align-items-center">
                <label for="agentNameInput" class="col-lg-3 col-3 col-form-label col-form-label-sm fw-bold">ผู้ขาย</label>
                <div class="col-lg-9 col-9">
                    <input type="text" readonly class="form-control-plaintext" id="agentNameInput" name="agentNameInput" placeholder="ผู้ขาย" value="@ViewBag.Name">
                </div>
            </div>
        </div>
    </div>

    <div class="row mb-3">
        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    <div class="row">
                        <div class="col-md-3 col-12">
                            <div class="row">
                                <label for="" class="col-lg-5 col-form-label fw-bold">ประเภทกรมธรรม์</label>
                                <div class="col-lg-7">
                                    <input type="text" readonly class="form-control-plaintext" id="policyTypeInput" name="policyTypeInput" placeholder="ประเภทกรมธรรม์" value="">
                                </div>
                            </div>
                        </div>
                        <div class="col-md-9 col-12">
                            <div class="row">
                                <label for="" class="col-lg-2 col-form-label fw-bold">ผลิตภัณฑ์</label>
                                <div class="col-lg-10">
                                    <input type="text" readonly class="form-control-plaintext" id="productNameInput" name="productNameInput" placeholder="ผลิตภัณฑ์" value="">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</header>

<content>
    <div class="row mb-3">
        <div class="col-auto">
            <button type="button" class="btn btn-outline-danger"
                    data-bs-target="#orderMotorModal"
                    data-bs-toggle="modal">
                <i class="fa-solid fa-magnifying-glass fa-lg"></i>
                <span class="align-middle">ค้นหาใบเสนอราคา</span>
            </button>
        </div>
    </div>

    <!-- Modal -->
    <section class="modal fade" id="orderMotorModal" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered modal-fullscreen-md-down modal-xl">
            <div class="modal-content">
                <div class="modal-header">
                    <h2 class="modal-title fs-5">
                        <i class="fa-solid fa-table-list fa-xl"></i>
                        รายการข้อมูลใบเสนอราคา
                    </h2>
                    <button type="button" id="closeOrderModalButton" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-12">
                            <div class="card">
                                <div class="card-header">
                                    <i class="fa-solid fa-filter fa-lg"></i> ค้นหาข้อมูล
                                </div>
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-lg-6 col-12 mb-3">
                                            <label for="registrationCarSearchInput" class="form-label">ทะเบียนรถ</label>
                                            <input type="text" class="form-control form-control-sm" id="registrationCarSearchInput" autocomplete="off" placeholder="ทะเบียนรถ">
                                        </div>
                                        <div class="col-lg-6 col-12 mb-3">
                                            <label for="customerSearchInput" class="form-label">ชื่อ-นามสกุล</label>
                                            <input type="text" class="form-control form-control-sm" id="customerSearchInput" autocomplete="off" placeholder="ชื่อ-นามสกุล">
                                        </div>
                                        <div class="col-lg-6 col-12 mb-3">
                                            <label for="quotationStartDateSearchInput" class="form-label">วันที่เริ่มทำรายการ</label>
                                            <input type="text" class="form-control form-control-sm" id="quotationStartDateSearchInput" readonly placeholder="วันที่เริ่มทำรายการ">
                                        </div>
                                        <div class="col-lg-6 col-12 mb-3">
                                            <label for="quotationEndDateSearchInput" class="form-label">วันที่สิ้นสุดทำรายการ</label>
                                            <input type="text" class="form-control form-control-sm" id="quotationEndDateSearchInput" readonly placeholder="วันที่สิ้นสุดทำรายการ">
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-12 mb-3">
                                            <div class="alert alert-warning" role="alert">
                                                <div class="row">
                                                    <div class="col-1 d-flex justify-content-center align-items-center text-center">
                                                        <i class="fa-solid fa-circle-exclamation fa-2xl text-danger"></i>
                                                    </div>
                                                    <div class="col-11">
                                                        <div class="card">
                                                            <div class="card-body">
                                                                <span>ใบเสนอราคานี้มีผลบังคับใช้ 30 วัน นับตั้งแต่วันที่ออกใบเสนอราคา</span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row justify-content-center">
                                        <div class="col-auto mb-3">
                                            <button class="btn btn-secondary" title="ค้นหา" id="quotationSearchButton"><i class="fa-solid fa-magnifying-glass fa-lg"></i> ค้นหา</button>
                                            <button class="btn btn-danger" title="เคลียร์" id="quotationResetSearchButton"><i class="fa-solid fa-rotate fa-lg"></i> เคลียร์</button>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-12">
                                            <div class="table-responsive">
                                                <table id="orderMotorTable" class="table align-middle table-striped dt-responsive nowrap" style="width:100%">
                                                    <thead class="table-warning">
                                                        <tr>
                                                            <th>ยี่ห้อ</th>
                                                            <th>รุ่น</th>
                                                            <th>ผลิตภัณฑ์</th>
                                                            <th class="text-end">เบี้ยรวม</th>
                                                            <th>วันทำรายการ</th>
                                                            <th>วันทำรายการ</th>
                                                            <th>แจ้งงาน</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody class="table-group-divider">
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <form id="newMotorSubmitForm" method="post" autocomplete="off">
        @Html.AntiForgeryToken()
        <div class="row mb-3">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">
                        <div class="row">
                            <div class="col-auto">
                                <i class="fa-solid fa-user fa-xl"></i>
                                <span class="me-2">ข้อมูลผู้เอาประกัน</span>
                            </div>
                            <div class="col-auto">
                                <div class="form-check form-check-inline">
                                    <input class="form-check-input" type="radio" name="customerTypeRadio" id="personRadio" value="N" checked>
                                    <label class="form-check-label" for="personRadio">บุคคล</label>
                                </div>
                            </div>
                            <div class="col-auto">
                                <div class="form-check form-check-inline">
                                    <input class="form-check-input" type="radio" name="customerTypeRadio" id="corporationRadio" value="Y">
                                    <label class="form-check-label" for="corporationRadio">นิติบุคคล</label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col-lg-2 col-6 mb-3">
                                <label for="preNameTitleSelect" class="form-label">คำนำหน้า</label>
                                <i class="fa-solid fa-star-of-life fa-2xs text-danger"></i>
                                <select class="form-select form-select-sm" id="preNameTitleSelect" name="preNameTitleSelect">
                                    <option selected value="">กรุณาเลือก</option>
                                </select>
                            </div>
                            <div class="col-lg-6 col-12 mb-3">
                                <div class="row">
                                    <div class="col-md-6 mb-2 mb-md-0" id="firstNameSection">
                                        <label for="firstNameInput" class="form-label">ชื่อ</label>
                                        <i class="fa-solid fa-star-of-life fa-2xs text-danger"></i>
                                        <input type="text" class="form-control form-control-sm" id="firstNameInput" name="firstNameInput" placeholder="ชื่อ">
                                    </div>
                                    <div class="col-md-6" id="lastNameSection">
                                        <label for="lastNameInput" class="form-label">นามสกุล</label>
                                        <i class="fa-solid fa-star-of-life fa-2xs text-danger"></i>
                                        <input type="text" class="form-control form-control-sm" id="lastNameInput" name="lastNameInput" placeholder="นามสกุล">
                                    </div>
                                    <div class="col-md-12 mb-2 mb-md-0" id="corporationSection" hidden>
                                        <label for="corporationNameInput" class="form-label">ชื่อนิติบุคคล</label>
                                        <i class="fa-solid fa-star-of-life fa-2xs text-danger"></i>
                                        <input type="text" class="form-control form-control-sm" id="corporationNameInput" name="corporationNameInput" placeholder="ชื่อนิติบุคคล">
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-4 col-12 mb-3">
                                <label for="citizenIdInput" class="form-label" id="customerIdLabel">เลขบัตรประชาชน</label>
                                <span class="messageErrorCardIDSpan"></span>
                                <i class="fa-solid fa-star-of-life fa-2xs text-danger"></i>
                                <input type="text" class="form-control form-control-sm" maxlength="13" id="citizenIdInput" name="citizenIdInput" placeholder="เลขบัตรประชาชน">
                                <div id="customerTypeCardSection">
                                    <div class="form-check form-check-inline col-form-label-sm">
                                        <input class="form-check-input" type="radio" name="customerTypeCardRadio" id="citizenIdRadio" value="01" checked>
                                        <label class="form-check-label" for="citizenIdRadio">บัตรประชาชน</label>
                                    </div>
                                    <div class="form-check form-check-inline col-form-label-sm">
                                        <input class="form-check-input" type="radio" name="customerTypeCardRadio" id="passportIdRadio" value="03">
                                        <label class="form-check-label" for="passportIdRadio">หนังสือเดินทาง (Passport)</label>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-2 col-md-6 col-12 mb-3" id="birthDateSection">
                                <label for="birthDateCustomerInput" class="form-label">วัน/เดือน/ปี <span class="text-secondary">(เกิด)</span></label>
                                <i class="fa-solid fa-star-of-life fa-2xs text-danger"></i>
                                <div class="input-group input-group-sm">
                                    <input type="text" class="form-control form-control-sm" id="birthDateCustomerInput" name="birthDateCustomerInput" readonly placeholder="วัน/เดือน/ปี (เกิด)">
                                    <span class="input-group-text" id="ageSpan">0</span>
                                    <span class="input-group-text">ปี</span>
                                </div>
                            </div>
                            <div class="col-lg-2 col-md-6 col-12 mb-3" id="registerDateSection" hidden>
                                <label for="registerDateCustomerInput" class="form-label">วัน/เดือน/ปี <span class="text-secondary">(จดทะเบียน)</span></label>
                                <i class="fa-solid fa-star-of-life fa-2xs text-danger"></i>
                                <div class="input-group input-group-sm">
                                    <input type="text" class="form-control form-control-sm" id="registerDateCustomerInput" name="registerDateCustomerInput" readonly placeholder="วัน/เดือน/ปี (จดทะเบียน)">
                                </div>
                            </div>
                            <div class="col-lg-3 col-md-6 col-12 mb-3">
                                <label for="mobileCustomerInput" class="form-label">โทรศัพท์มือถือ</label>
                                <i class="fa-solid fa-star-of-life fa-2xs text-danger"></i>
                                <input type="text" class="form-control form-control-sm numberOnly" maxlength="10" id="mobileCustomerInput" name="mobileCustomerInput" placeholder="โทรศัพท์มือถือ">
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-12">
                                <div class="alert alert-danger align-content-center" role="alert">
                                    <div class="row">
                                        <div class="col-1 d-flex justify-content-center align-items-center  text-center">
                                            <i class="fa-solid fa-circle-exclamation fa-2xl text-danger"></i>
                                        </div>
                                        <div class="col-11">
                                            <div class="card">
                                                <div class="card-body">
                                                    <span>ท่านสามารถตรวจสอบเอกสารยืนยันการรับแจ้งประกัน</span>
                                                    <i class="fa-solid fa-file-pdf fa-xl"></i>
                                                    <span>โดยใช้รหัสผ่านเป็น 4 หลักท้ายของ</span>
                                                    <span id="usePasswordBySpan">เลขบัตรประชาชน</span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="row mb-3">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">
                        <i class="fa-solid fa-address-card fa-xl"></i>
                        <span class="me-2">ข้อมูลที่อยู่ผู้เอาประกันภัย</span>
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col-lg-4 col-12 mb-3">
                                <label for="customerAddressNoInput" class="form-label">เลขที่</label>
                                <i class="fa-solid fa-star-of-life fa-2xs text-danger"></i>
                                <input type="text" class="form-control form-control-sm" maxlength="50" id="customerAddressNoInput" name="customerAddressNoInput" placeholder="เลขที่">
                                <label class="text-danger col-form-label-sm" for="" data-bs-toggle="tooltip" data-bs-placement="bottom" data-bs-title="59/5 อาคารพาราไดซ์ ถ.ศรีนครินทร์">ตัวอย่าง <i class="fa-solid fa-circle-info"></i></label>
                            </div>
                            <div class="col-lg-4 col-md-6 col-12 mb-3">
                                <label for="customerAddressProvinceSelect" class="form-label">จังหวัด</label>
                                <i class="fa-solid fa-star-of-life fa-2xs text-danger"></i>
                                <select class="form-select form-select-sm" id="customerAddressProvinceSelect" name="customerAddressProvinceSelect">
                                    <option selected value="">กรุณาเลือก</option>
                                </select>
                            </div>
                            <div class="col-lg-4 col-md-6 col-12 mb-3">
                                <label for="customerAddressDistrictSelect" class="form-label">อำเภอ/เขต</label>
                                <i class="fa-solid fa-star-of-life fa-2xs text-danger"></i>
                                <select class="form-select form-select-sm" id="customerAddressDistrictSelect" name="customerAddressDistrictSelect">
                                    <option selected value="">กรุณาเลือก</option>
                                </select>
                            </div>
                            <div class="col-lg-4 col-md-6 col-12 mb-3">
                                <label for="customerAddressSubDistrictSelect" class="form-label">ตำบล/แขวง</label>
                                <i class="fa-solid fa-star-of-life fa-2xs text-danger"></i>
                                <select class="form-select form-select-sm" id="customerAddressSubDistrictSelect" name="customerAddressSubDistrictSelect">
                                    <option selected value="">กรุณาเลือก</option>
                                </select>
                            </div>
                            <div class="col-lg-4 col-md-6 col-12 mb-3">
                                <label for="customerAddressZipCodeInput" class="form-label">รหัสไปรษณีย์</label>
                                <i class="fa-solid fa-star-of-life fa-2xs text-danger"></i>
                                <input type="text" class="form-control form-control-sm numberOnly" id="customerAddressZipCodeInput" name="customerAddressZipCodeInput" maxlength="5" placeholder="รหัสไปรษณีย์">
                            </div>
                            <div class="col-lg-4 col-md-6 col-12 mb-3">
                                <label for="customerOccupationSelect" class="form-label">อาชีพ</label>
                                <i class="fa-solid fa-star-of-life fa-2xs text-danger"></i>
                                <select class="form-select form-select-sm" id="customerOccupationSelect" name="customerOccupationSelect">
                                    <option selected value="">กรุณาเลือก</option>
                                </select>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="row mb-3">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">
                        <div class="row">
                            <div class="col-md-auto col-12">
                                <i class="fa-solid fa-envelope-open-text fa-xl"></i>
                                <span class="me-2">ข้อมูลที่อยู่ในการจัดส่งเอกสาร</span>
                            </div>
                            <div class="col-md-auto col-12">
                                <div class="form-check form-check-inline">
                                    <input class="form-check-input" type="radio" name="sendByAddressRadio" id="sendForAgentRadio" value="A" checked>
                                    <label class="form-check-label" for="sendForAgentRadio">
                                        ตามที่อยู่ปัจจุบันนายหน้า
                                    </label>
                                </div>
                            </div>
                            <div class="col-md-auto col-12">
                                <div class="form-check form-check-inline">
                                    <input class="form-check-input" type="radio" name="sendByAddressRadio" id="sendForCustomerRadio" value="I">
                                    <label class="form-check-label" for="sendForCustomerRadio">
                                        ตามที่อยู่ผู้เอาประกันภัย
                                    </label>
                                </div>
                            </div>
                            <div class="col-lg-auto col-12">
                                <div class="form-check form-switch form-check-inline">
                                    <input class="form-check-input" type="checkbox" value="" id="editAddressCheckbox">
                                    <label class="form-check-label" for="editAddressCheckbox">
                                        แก้ไขที่อยู่
                                    </label>
                                    <span class="form-text text-muted" id="editAddressBySpan">ตามที่อยู่ปัจจุบันนายหน้า</span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col-lg-4 col-12 mb-3">
                                <label for="sendAddressNoInput" class="form-label">เลขที่</label>
                                <i class="fa-solid fa-star-of-life fa-2xs text-danger"></i>
                                <input type="text" class="form-control form-control-sm readonly" maxlength="50" id="sendAddressNoInput" name="sendAddressNoInput" placeholder="เลขที่" readonly>
                                <label class="text-danger col-form-label-sm" for="" data-bs-toggle="tooltip" data-bs-placement="bottom" data-bs-title="59/5 อาคารพาราไดซ์ ถ.ศรีนครินทร์">ตัวอย่าง <i class="fa-solid fa-circle-info"></i></label>
                            </div>
                            <div class="col-lg-4 col-md-6 col-12 mb-3">
                                <label for="sendAddressProvinceSelect" class="form-label">จังหวัด</label>
                                <i class="fa-solid fa-star-of-life fa-2xs text-danger"></i>
                                <select class="form-select form-select-sm readonly" id="sendAddressProvinceSelect" name="sendAddressProvinceSelect">
                                    <option selected value="">กรุณาเลือก</option>
                                </select>
                            </div>
                            <div class="col-lg-4 col-md-6 col-12 mb-3">
                                <label for="sendAddressDistrictSelect" class="form-label">อำเภอ/เขต</label>
                                <i class="fa-solid fa-star-of-life fa-2xs text-danger"></i>
                                <select class="form-select form-select-sm readonly" id="sendAddressDistrictSelect" name="sendAddressDistrictSelect">
                                    <option selected value="">กรุณาเลือก</option>
                                </select>
                            </div>
                            <div class="col-lg-4 col-md-6 col-12 mb-3">
                                <label for="sendAddressSubDistrictSelect" class="form-label">ตำบล/แขวง</label>
                                <i class="fa-solid fa-star-of-life fa-2xs text-danger"></i>
                                <select class="form-select form-select-sm readonly" id="sendAddressSubDistrictSelect" name="sendAddressSubDistrictSelect">
                                    <option selected value="">กรุณาเลือก</option>
                                </select>
